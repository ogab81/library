<%- include ('_header') %>

<div class='container'>
<h3 style="color: gray; font-family: Roboto;font-weight: bold;">Update a record</h3><br>

<form action="/staff-edit/<%= model.Book_ID %>" method="POST">
    <div class="form-horizontal">

        <%- include ('_editor') %>

        <div class="form-group row">
            <label class="col-form-label co-sm-2"></label>
            <div class="col-sm-10">
                <input type="submit" value="Update" class ="btn btn-default btn-warning"/>
                <a class =" btn btn-outline-dark cancel" href="/staff-view">Cancel</a>
            </div>
        </div>
    </div>
</form>

<%- include ('_footer') %>